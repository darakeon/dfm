@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section HtmlHeadCss {
    <link href="@Url.Content("~/Assets/Css/Menu.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Css/Table.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Css/SystemIn.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Css/Navigation.css")" rel="stylesheet" type="text/css" />

    @RenderSection("HtmlHeadCss", required: false)
}

@section HtmlHeadJs {
    @RenderSection("HtmlHeadJs", required: false)
}

@section LogOut {
    <img id="Out" alt="DFM" src="@Url.Content("~/Assets/Image/off_in.gif")"
        onmouseover="this.src='@Url.Content("~/Assets/Image/off_in_hover.gif")'"
        onmouseout="this.src='@Url.Content("~/Assets/Image/off_in.gif")'" />
}

@section Logo {
    <img alt="DFM" src="@Url.Content("~/Assets/Image/logo_in.gif")" />
}

@section Menu { 
    @{ Html.RenderPartial("Inner/Menu"); }
}

@{/* Made without spaces to hide when empty */}
@section TopRightDiv {@RenderSection("TopRightDiv", required: false)}

@RenderBody()