@model DFM.MVC.Areas.Accounts.Models.ReportSeeYearModel

@{
    ViewBag.Title = String.Format("{0}: {1} Summary", Model.Account, Model.Year);
    Layout = "~/Views/Shared/Inner/_Layout.cshtml";
}

@if (Model.MoveSumList.Count == 0) { 
    
    @PlainText.Dictionary["NoYearMoves"]
        
} else { 
        
    <table class="List">

        <tr>
            <td>@PlainText.Dictionary["Month"]</td>
            <td>@PlainText.Dictionary["Value"]</td>
        </tr>

        @foreach (var move in Model.MoveSumList) { 
            
            <tr>
                <td>@move.Key </td>
                <td>@move.Value </td>
            </tr>

        } 

    </table>
} 
