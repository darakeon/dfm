@model AccountSign
@switch (Model)
{
	case AccountSign.GreenLimit:
		@("thumbs-up")
		break;
	case AccountSign.YellowLimit:
		@("thumbs-down")
		break;
	case AccountSign.RedLimit:
		@("fire")
		break;
	case AccountSign.NoLimit:
		@("minus")
		break;
	default:
		throw new NotImplementedException();
}
