@model UsersAskRemoveTFAModel

@{
	ViewBag.Title = Context.Translate("RemoveTFA");
	Layout = "~/Views/Shared/Layouts/_Form.cshtml";
}

<div class="form-group">
	@Html.LabelFor(m => m.Password, Context.Translate("Password"), new { @class = "control-label" })
	@Html.PasswordFor(m => m.Password, new { @class = "form-control" })
</div>

@section Footer
{
	<button type="submit" class="btn btn-success">
		@Context.Translate("Send")
	</button>
}
