@model BaseMovesModel

@{
	var route = new RouteDefault();
}

@if (Model.ShowNoAccounts)
{
	@Html.RouteLink(Context.Translate("Accounts"),
		route.Name, new { controller = "Accounts" },
		new { @class = "btn btn-success" })
}

@if (Model.ShowNoCategories)
{
	@Html.RouteLink(Context.Translate("Categories"),
		route.Name, new { controller = "Categories" },
		new { @class = "btn btn-success" })
}

