@model DFM.MVC.Models.OpsCodeModel
@using DFM.Email;

@{
    ViewBag.Title = MultiLanguage.Dictionary["Ops"];
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.EmailSent == Error.Status.Sent)
{
    @MultiLanguage.Dictionary["InternalMessageSent"]
}
else
{
    @MultiLanguage.Dictionary["InternalMessageNotSent"]
}

