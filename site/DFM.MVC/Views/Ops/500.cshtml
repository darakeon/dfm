@model OpsCodeModel
@using DFM.Email;

@{
	ViewBag.Title = Translator.Dictionary["Ops"];
	Layout = "~/Views/Shared/_result.cshtml";

	var messageKey = Model.EmailSent == Error.Status.Sent
		? "InternalMessageSent"
		: "InternalMessageNotSent";

	ViewBag.Style = "danger";
}

@(Translator.Dictionary[messageKey])

