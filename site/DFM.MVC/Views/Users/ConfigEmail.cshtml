@model UsersConfigModel

<div class="form-group">
	@Html.LabelFor(m => m.Info.Email, MultiLanguage.Dictionary["EmailToChange"], new { @class = "control-label" })
	@Html.TextBoxFor(m => m.Info.Email, new { @class = "form-control" })
</div>

<div class="form-group">
	@Html.LabelFor(m => m.Info.CurrentPassword, MultiLanguage.Dictionary["CurrentPassword"], new { @class = "control-label" })
	@Html.PasswordFor(m => m.Info.CurrentPassword, new { @class = "form-control" })
</div>
