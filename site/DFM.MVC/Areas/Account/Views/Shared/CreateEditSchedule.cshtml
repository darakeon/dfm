@model BaseMovesModel

@{
	ViewBag.Title = MultiLanguage.Dictionary[Model.ActionName, Model.ControllerName];
	Layout = "~/Views/Shared/_Form.cshtml";
	ViewBag.FormSize = 4;
}

@section Scripts {
	<script src="/Assets/Scripts/toggle.js" type="text/javascript"></script>
	<script src="/Assets/Scripts/move-crud.js" type="text/javascript"></script>
}

@Html.Partial("MoveMainData")

@Html.Partial("Nature")

@Html.Partial("Account")

@Html.Partial("ValueAndDetail")

@section Footer {
	<button type="submit" class="btn btn-success">
		@MultiLanguage.Dictionary[Model.ActionName]
	</button>
}